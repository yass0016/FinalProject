        <activity android:name=".kitchen.KitchenMainActivity" />
        <activity android:name=".kitchen.KitchenDeviceConfig" />

        <activity android:name=".House.House"/>
        <activity android:name=".House.GarageFragment"/>
        <activity android:name=".House.TempInFragment"/>
        <activity android:name=".House.TempOutFragment"/>
    </application>

</manifest>